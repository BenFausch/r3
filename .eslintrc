{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true,
    "jquery": true,
  },
  "rules": {
    /**
     * General
     */
    "no-var": 0,
    "strict": 0,

    /**
     * Allowances
     */
    "block-scoped-var": 0,
    "dot-notation": 0,
    //"eqeqeq": [2, "smart"],
    "no-console": 0,
    "no-param-reassign": 0,
    "no-reserved-keys": 0,
    "no-undef": 0,
    "radix": 0,
    "yoda": 0,
    "vars-on-top": 0,

    /**
     * Errors
     */
    "no-cond-assign": [2, "always"],
    "no-constant-condition": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-else-return": 0,
    "no-empty": 2,
    "no-eq-null": 2,
    "no-eval": 2,
    "no-ex-assign": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-extra-semi": 2,
    "no-func-assign": 2,
    "no-implied-eval": 2,
    "no-inner-declarations": 2,
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 2,
    "no-lone-blocks": 2,
    "no-loop-func": 0,
    "no-native-reassign": 2,
    "no-new": 2,
    "no-new-func": 2,
    "no-new-wrappers": 2,
    "no-obj-calls": 2,
    "no-octal": 2,
    "no-octal-escape": 2,
    "no-proto": 2,
    "no-redeclare": 2,
    "no-return-assign": 2,
    "no-script-url": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-shadow": [1, {"hoist": "functions"}],
    "no-sparse-arrays": 2,
    "no-throw-literal": 2,
    "no-unreachable": 2,
    "no-with": 2,
    "use-isnan": 0,
    "wrap-iife": [2, "any"],

    /**
     * Warnings
     */
    "consistent-return": 1,
    "default-case": 1,
    "guard-for-in": 1,
    //"max-len": [1, 80, 4],
    "no-alert": 0,
    "no-caller": 1,
    "no-debugger": 1,
    "no-extra-boolean-cast": 1,
    "no-fallthrough": 1,
    "no-floating-decimal": 1,
    "no-multi-spaces": 0,
    "no-multi-str": 1,
    "no-shadow-restricted-names": 2,
    "no-unused-vars": [1, {
      "vars": "local",
      "args": "none"
    }],
    "no-use-before-define": 2,

    /**
     * Style
     */
    "brace-style": [2,
      "1tbs", {
      "allowSingleLine": true
    }],
    /*"camelcase": [2, {
      "properties": "always"
    }],*/
    "comma-spacing": [2, {
      "before": false,
      "after": true
    }],
    "curly": 0,
    "comma-style": [2, "last"],
    "eol-last": 1,
    "func-names": 0,
    "func-style": 0,
    "indent": 1,
    "key-spacing": [2, { "align": "value", "mode": "minimum" }],
    "linebreak-style": [
      2,
      "unix"
    ],
    "new-cap": 2,
    "no-multiple-empty-lines": 0,
    "no-nested-ternary": 0,
    "no-new-object": 2,
    "no-array-constructor": 2,
    "no-spaced-func": 2,
    "no-trailing-spaces": [2,{ "skipBlankLines": true }],
    "no-underscore-dangle": 0,
    "one-var": [2, "never"],
    "padded-blocks": [0, "always"],
    "quotes": [
      2, "single", "avoid-escape"
    ],
    "semi": [2, "always"],
    "semi-spacing": [2, {
      "before": false,
      "after": true
    }],
    "keyword-spacing": [2, { "before": true, "after": true }],
    "space-before-blocks": 2,
    "space-infix-ops": 2,
  }
}